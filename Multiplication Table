import java.util.Scanner;
 class Demo
 {
 	 public static void main(String args[])
 	  { 
 	   Scanner t= new Scanner(System.in); 
 	   int number = t.nextInt();  for(int a = 1; a <= 10; a++)
 {
 System.out.println(number + " * " + a + " = " + number * a); 
 } 
  } 
  }
OUTPUT:
3
3 * 1 = 3
3 * 2 = 6
3 * 3 = 9
3 * 4 = 12
3 * 5 = 15
3 * 6 = 18
3 * 7 = 21
3 * 8 = 24
3 * 9 = 27
3 * 10 = 30

